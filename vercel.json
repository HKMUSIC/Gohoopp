{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
{
  "builds":[
    {
      "src":"shivu/web/backend/app.py",
      "use":"@vercel/python"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"shivu/web/backend/app.py"
    }
  ]
}
