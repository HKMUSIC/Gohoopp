{
  "version": 2,
  "builds": [
    {
      "src": "shivu/web/backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "shivu/web/frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "shivu/web/backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "shivu/web/frontend/index.html"
    }
  ]
}
